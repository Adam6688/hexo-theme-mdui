<!DOCTYPE html>
<html lang="<%=config.language%>">
<%-partial('partials/share/head')%>
<% if (theme.style.scheme === '1') { %>
  <body class="mdui-theme-primary-<%= theme.color.primary_color %> mdui-theme-accent-<%= theme.color.accent_color %> <% if (theme.color.layout === 'dark') { %>mdui-theme-layout-dark<% } %>" id="top">
  <%- partial('partials/1/appbar') %>
  <%- partial('partials/1/parallax_card') %>
  <main class="mdui-center style-1-top style-1-container">
    <%- body %>
  </main>
<% } else { %>
  <body class="mdui-appbar-with-toolbar mdui-theme-primary-<%= theme.color.primary_color %> mdui-theme-accent-<%= theme.color.accent_color %> <% if (theme.color.layout === 'dark') { %>mdui-theme-layout-dark<% } %>" id="top">
  <%- partial('partials/0/appbar') %>
  <main class="mdui-container">
    <br />
    <%- body %>
  </main>
<% } %>

<%- partial('partials/share/drawer') %>
<div id="outdated">
  <h6><%= theme.translations.outdated_browser.title %></h6>
  <p><%= theme.translations.outdated_browser.line_one %><a id="btnUpdateBrowser" href="http://outdatedbrowser.com/"><%= theme.translations.outdated_browser.link %></a></p>
  <p class="last"><a href="#" id="btnCloseUpdateBrowser" title="<%= theme.translations.outdated_browser.close %>">&times;</a></p>
</div>
<%- partial('partials/share/footer') %>
</body>
<%- partial('partials/share/import_js') %>
</html>
