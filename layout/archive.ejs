<% if (!is_year()) { %>
  <%- partial('partials/archives/index.ejs') %>
<% } else { %>
  <div class="posts-container column-3">

  </div>
  <div id="content-list">
    <% page.posts.each(function(post,index) {%>
      <div class="content-item">
        <% if(index <= 2) { %>
          <div class="posts-header mdui-center" style="max-width: 498px;">
            <%= theme.translations.posts.header %>
          </div>
        <% } %>
        <% if(!(theme.post.topPost && post.top)) { %>
          <%-partial('partials/posts', { post: post, index: true, pin: false })%>
        <% } %>
      </div>
    <% }); %>
  </div>
  <%-partial('partials/pagination', {base: url_for(config.root) + 'archives/' + page.year + '/', show: theme.pagination })%>
<% } %>
<%-partial('partials/fab/index')%>
