<% if (!is_year()) { %>
<%- partial('partials/archives/index.ejs') %>
<% } else { %>
  <% page.posts.each(function(post) { %>
      <%-partial('partials/posts', { post: post })%>
  <% }); %>
  <%-partial('partials/pagination', {base: url_for(config.root) + 'archives/' + page.year + '/' + page.month + '/', show: theme.index.show })%>
<% } %>
