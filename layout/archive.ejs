<% if (!is_year()) { %>
<%- partial('partials/share/archives/index.ejs') %>
<% } else { %>
  <% page.posts.each(function(post) { %>
      <%-partial('partials/share/posts', { post: post })%>
  <% }); %>
  <%-partial('partials/share/pagination', {base: url_for(config.root) + 'archives/' + page.year + '/', show: theme.index.show })%>
<% } %>
<%-partial('partials/share/fab/index')%>
