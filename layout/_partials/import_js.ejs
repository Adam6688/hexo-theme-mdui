<!-- import js -->
<%- js('js/js.min') %>

<!-- theme js -->
<%-partial('js/appbar')%>

<% if (theme.service_woker === true || theme.service_woker === 'pwa') { %>
  <script>
    if ('serviceWorker' in navigator) {
     navigator.serviceWorker.register('<%= config.root + 'sw.js' %>');
    }
  </script>
<% } %>

<% if (theme.vendors.custom_js === true) { %>
  <!-- Custom JS -->
  <%-partial('../_custom/import_js')%>
<% } %>

<script type="text/javascript">
  document.querySelector('body').classList.add('theme-ls-loaded')
</script>
